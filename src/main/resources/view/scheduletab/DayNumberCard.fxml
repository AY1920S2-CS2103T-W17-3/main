<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER_LEFT" minHeight="12" GridPane.columnIndex="0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="5" left="15" right="5" top="5"/>
    </padding>
    <HBox alignment="CENTER_LEFT" spacing="5">
        <Label styleClass="cell_big_label" text="Day">
            <minWidth>
                <Region fx:constant="USE_PREF_SIZE"/>
            </minWidth>
        </Label>
        <Label fx:id="dayNumber" styleClass="cell_big_label" text="\$title" wrapText="true"/>
    </HBox>
</VBox>
