@startuml
!include style.puml

hide member
skinparam Class {
    BorderColor BACKGROUND_COLOR
    FontColor BACKGROUND_COLOR
    BackgroundColor MODEL_COLOR_T2
    StereotypeFontColor BACKGROUND_COLOR
}
skinparam ArrowColor MODEL_COLOR_T2

Class HiddenOutside BACKGROUND_COLOR

Package Model <<Rectangle>> {
Class ReadOnlyXYZ <<Interface>>
Class Model <<Interface>>
Class ObservableList <<Interface>>
Class XYZManager
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs
}

HiddenOutside ..> Model

XYZManager .|> ReadOnlyXYZ
ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "6 " XYZManager
ModelManager o-left-> " 1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs
@enduml
